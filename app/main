# Write Python 3 code in this online editor and run it.
number = [2,4,5,12,45,34,56]
unique = list(set(number))
total = sum(unique)
print("total sum = ", total)
maximum = max(number)
minimum = min(number)
print("max number", maximum)
print("min mumber", minimum)
number.sort()
print("second max", number[-2])
print("second min", number[1])
square = [x**2 for x in number if x % 2 == 0]
print("number square", square)

fruits = ["apple","banana","mango","orange"]
new_fruit = input("enter fruit name:").lower()
if new_fruit in fruits:
    print(f"{new_fruit} is available at index {fruit.index(new_fruit)}")
else:
    print(f"{new_fruit} is not avvailable")
